#footer_muestra{
              background: rgba(0, 0, 0, 0.698);
              width: 100%;
              height: 100%;
              position: absolute;
              z-index: 10;
              
            }
            
            #formulario_muestra{
              background:#EDF2F5;
              position: absolute;
              z-index: 100;
              top:3%;
              left:25%;
              width:45%;
              height: auto;
              border-radius: 30px;
              border:medium solid rgb(0,43,87);    
